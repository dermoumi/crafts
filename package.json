{
  "name": "@crafts/monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "build": "nx run-many --parallel 8 -t build",
    "dev": "pnpm -r --parallel run dev",
    "lint": "nx run-many --parallel 8 -t lint",
    "test": "nx run-many --parallel 8 -t test",
    "test:cov": "nx run-many --parallel 8 --output-style=static -t test:cov",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "nx": "^15.8.8"
  }
}
